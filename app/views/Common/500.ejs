<body>
   <div id="error">
      <div class="container-fluid">
         <!-- 404 Error Text -->
         <div class="text-center ">
            <div class="error mx-auto" data-text="500">500</div>
            <p class="lead text-gray-800 mb-5  error-title">Internal Server Error</p>
            <% if (err.errno) { %>
               <p class="text-gray-500 mb-0 error-body">ErrorNo: <%= err.errno %></p>
            <% } %>
            <% if (err.code) { %>
               <p class="text-gray-500 mb-0 error-body">Code: <%= err.code %></p>
             
            <% } %>
            <div class="stack text-left">
               <h4>Error Message:</h4>
               <pre class="pre-scrollable">
                  <code><%= err.message %></code>
               </pre>
           
               <h4>Stack Trace</h4>
               <pre class="pre-scrollable">
                  <code><%= stack %></code>
               </pre>
            </div>
            <br><br>
            <a href="javascript:history.back()" class="error-body">‚Üê Go Back</a>
         </div>
      </div>
      <div class="footer pt-32">
         <p class="text-center">Copyright &copy; Rivera's Group <%= new Date().getFullYear()%></p>
      </div>
   </div>
